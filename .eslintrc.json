{
  "env": {
    "browser": false,
    "commonjs": true,
    "es2021": true
  },
  "extends": ["eslint:recommended", "plugin:@typescript-eslint/recommended"],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 2020
  },
  "plugins": ["@typescript-eslint"],
  "rules": {
    "no-unused-vars": "warn"
    // "no-var-requires": "false"
  },
  "ignore": ["./src/response"]
}
